#include <iostream> 
#include<deque>
using namespace std; 
int main() 
{ deque<int>dq; dq.push_back(10); dq.push_front(20); cout<<dq.size()<<endl; cout<<dq.front()<<endl; cout<<dq.back()<<endl; cout<<dq.empty()<<endl<<endl; dq.pop_front(); cout<<dq.size()<<endl; cout<<dq.front()<<endl; cout<<dq.back()<<endl; cout<<dq.empty()<<endl<<endl; dq.pop_front(); cout<<dq.size()<<endl; cout<<dq.front()<<endl; cout<<dq.back()<<endl; cout<<dq.empty()<<endl<<endl; dq.pop_back(); cout<<dq.size()<<endl; cout<<dq.front()<<endl; cout<<dq.back()<<endl; cout<<dq.empty()<<endl; 
return 0; }
